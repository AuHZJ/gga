/* pages/setting/setting.wxss */
.backgroundImage{
  width: 100%;
  height: 100%;
  z-index: -1;
  position: fixed;
}
.privacy,.currency,.about{
  width: 100%;
  height: 55px;
  line-height: 55px;
  font-size: 17px;
  color: rgba(0,0,0,0.5);
  padding-left: 20px;
  border-bottom: 1px solid rgba(0,0,0,0.1);
}
.privacy::after,.currency::after,.about::after{
  content: '>';
  position: absolute;
  right:25px;
  font-size: 25px;
  color: rgba(0,0,0,0.2);
}
button{
  background-color: #ff6b69;
  position: absolute;
  bottom: 0;
  width: 100%;
  color: white;
  border: none;
  border-radius: 0px;
}