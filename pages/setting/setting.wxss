.backgroundImage{
  width: 100%;
  height: 100%;
  z-index: -1;
  position: fixed;
}
.privacy,.currency,.about{
  position: relative;
  width: 100%;
  height: 55px;
  line-height: 55px;
  font-size: 17px;
  color: rgba(0,0,0,0.5);
  padding-left: 20px;
