/* pages/homepage/homepage.wxss */
.backgroundImage{
  width: 100%;
  height: 100%;
  z-index: -1;
  position: fixed;
}
.body{
  position: relative;
  width: 100%;
  height: 100%;
  text-align: center;
  padding-top: 100px;
}
.info{
  position: absolute;
  right: 0px;
  top: 0px;
}
.redDot{
  position: absolute;
  right: 10px;
  top: 13px;
  width: 10px;
  height: 10px;
  border-radius: 5px;
  background: red;
}
.info image{
  width: 50px;
  height: 50px;
}
.title{
  display: block;
  font-family: FangSong_GB2312;
  font-size: 18px;
  color: white;
}
.logo{
  margin-top: 50px;
  width: 250px;
  height: 250px;
}